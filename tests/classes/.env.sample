# Environment variables for DomoAccount tests
# Copy this file to .env and fill in your actual values

# Domo instance authentication
DOMO_INSTANCE=your-instance
DOMO_ACCESS_TOKEN=your-access-token

# Test account IDs (find these in your Domo instance)
# Navigate to Data Center > Accounts to find account IDs
TEST_ACCOUNT_ID=123

# Optional: Additional test accounts for comprehensive testing
TEST_ACCOUNT_ID_2=456
TEST_OAUTH_ACCOUNT_ID=789

# Optional: For cross-instance testing
DOMO_DOJO_INSTANCE=dojo-instance
DOMO_DOJO_ACCESS_TOKEN=dojo-token
